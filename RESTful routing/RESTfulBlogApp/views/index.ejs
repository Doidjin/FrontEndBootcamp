<!-- Including my templates -->
<% include partials/header %>

<% blogs.forEach(function(blog){ %>
    <div>
        <h2><%= blog.title %></h2>
        <img src="<%= blog.image %>">
        <h2><%= blog.created %></h2>
        <p><%= blog.body %></p>
    </div>
<% }); %>

<% include partials/footer %>